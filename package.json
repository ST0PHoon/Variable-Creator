{
  "name": "variable-creator",
  "displayName": "YJY-TEST-variable creator",
  "description": "variable creator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:variable-creator.translateInsert",
    "onCommand:variable-creator.translate"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "variable-creator.translateInsert",
        "title": "Translate & Insert"
      },
      {
        "command": "variable-creator.translate",
        "title": "Translate"
      }
    ],
    "keybindings": [
      {
        "command": "variable-creator.translateInsert",
        "key": "alt+t",
        "when": "editorTextFocus"
      },
      {
        "command": "variable-creator.translate",
        "key": "ctrl+t",
        "when": "editorHasSelection"
      }
	],
	"configuration": {
		"title": "Translator Helper",
		"properties": {
			"variable-creator.api": {
				"type": "string",
				"default": "google",
				"enum": [
					"google"
				],
				"enumDescriptions": [
					"Google Translation API."
				],
				"description": "Specify the api to translate the text."
			},
			"variable-creator.sourceLanguage": {
				"type": "string",
				"default": "ko",
				"description": "The source language to be translated."
			},
			"variable-creator.targetLanguage": {
				"type": "string",
				"default": "en",
				"description": "The target language."
			}
		}
	}
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0"
  }
}
